      </markerHelp>
      <markerHelp
            markerType="org.thenesis.emulation.eclipse.readmemarker"
            helpContextId="org.thenesis.emulation.eclipse.marker_example6_context">
         <attribute
               name="org.thenesis.emulation.eclipse.id"
               value="1234">
         </attribute>
         <attribute
               name="org.thenesis.emulation.eclipse.level"
               value="7">
         </attribute>
         <attribute
               name="org.thenesis.emulation.eclipse.department"
               value="infra">
         </attribute>
         <attribute
               name="org.thenesis.emulation.eclipse.code"
               value="red">
         </attribute>
         <attribute
               name="org.thenesis.emulation.eclipse.language"
               value="english">
         </attribute>
      </markerHelp>
      <markerHelp
            markerType="org.thenesis.emulation.eclipse.readmemarker"
            helpContextId="org.thenesis.emulation.eclipse.marker_example7_context">
      </markerHelp>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            markerType="org.thenesis.emulation.eclipse.readmemarker"
            class="org.thenesis.emulation.eclipse.ReadmeMarkerResolutionGenerator">
         <attribute
               name="org.thenesis.emulation.eclipse.id"
               value="1234">
         </attribute>
      </markerResolutionGenerator>
      <markerResolutionGenerator
            markerType="org.thenesis.emulation.eclipse.readmemarker"
            class="org.thenesis.emulation.eclipse.ReadmeMarkerResolutionGenerator">
         <attribute
               name="org.thenesis.emulation.eclipse.level"
               value="7">
         </attribute>
      </markerResolutionGenerator>
      <markerResolutionGenerator
            markerType="org.thenesis.emulation.eclipse.readmemarker"
            class="org.thenesis.emulation.eclipse.ReadmeMarkerResolutionGenerator">
